<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tenant.business.mapper.TbDrawManualMapper">
    
    <resultMap type="TbDrawManual" id="TbDrawManualResult">
        <result property="id"    column="id"    />
        <result property="roomName"    column="room_name"    />
        <result property="periodNumber"    column="period_number"    />
        <result property="first"    column="first"    />
        <result property="second"    column="second"    />
        <result property="third"    column="third"    />
        <result property="delFlag"    column="del_flag"    />
        <result property="createBy"    column="create_by"    />
        <result property="createTime"    column="create_time"    />
        <result property="updateBy"    column="update_by"    />
        <result property="updateTime"    column="update_time"    />
        <result property="remark"    column="remark"    />
        <result property="tenantId"    column="tenant_id"    />
    </resultMap>

    <sql id="selectTbDrawManualVo">
        select * from tb_draw_manual
    </sql>

    <select id="selectTbDrawManualList" parameterType="TbDrawManual" resultMap="TbDrawManualResult">
        <include refid="selectTbDrawManualVo"/>
        <where>  
            <if test="id != null "> and id = #{id}</if>
            <if test="roomName != null  and roomName != ''"> and room_name = #{roomName}</if>
            <if test="createBy != null  and createBy != ''"> and create_by like concat('%', #{createBy}, '%')</if>
            <if test="params.beginCreateTime != null and params.beginCreateTime != '' and params.endCreateTime != null and params.endCreateTime != ''"> and create_time between #{params.beginCreateTime} and #{params.endCreateTime}</if>
            <if test="tenantId != null "> and tenant_id = #{tenantId}</if>
        </where>
    </select>
    
    <select id="selectTbDrawManualById" parameterType="Long" resultMap="TbDrawManualResult">
        <include refid="selectTbDrawManualVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTbDrawManual" parameterType="TbDrawManual" useGeneratedKeys="true" keyProperty="id">
        insert into tb_draw_manual
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="roomName != null">room_name,</if>
            <if test="periodNumber != null">period_number,</if>
            <if test="first != null">first,</if>
            <if test="second != null">second,</if>
            <if test="third != null">third,</if>
            <if test="delFlag != null">del_flag,</if>
            <if test="createBy != null">create_by,</if>
            <if test="createTime != null">create_time,</if>
            <if test="updateBy != null">update_by,</if>
            <if test="updateTime != null">update_time,</if>
            <if test="remark != null">remark,</if>
            <if test="tenantId != null">tenant_id,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="roomName != null">#{roomName},</if>
            <if test="periodNumber != null">#{periodNumber},</if>
            <if test="first != null">#{first},</if>
            <if test="second != null">#{second},</if>
            <if test="third != null">#{third},</if>
            <if test="delFlag != null">#{delFlag},</if>
            <if test="createBy != null">#{createBy},</if>
            <if test="createTime != null">#{createTime},</if>
            <if test="updateBy != null">#{updateBy},</if>
            <if test="updateTime != null">#{updateTime},</if>
            <if test="remark != null">#{remark},</if>
            <if test="tenantId != null">#{tenantId},</if>
         </trim>
    </insert>

    <update id="updateTbDrawManual" parameterType="TbDrawManual">
        update tb_draw_manual
        <trim prefix="SET" suffixOverrides=",">
            <if test="roomName != null">room_name = #{roomName},</if>
            <if test="periodNumber != null">period_number = #{periodNumber},</if>
            <if test="first != null">first = #{first},</if>
            <if test="second != null">second = #{second},</if>
            <if test="third != null">third = #{third},</if>
            <if test="delFlag != null">del_flag = #{delFlag},</if>
            <if test="createBy != null">create_by = #{createBy},</if>
            <if test="createTime != null">create_time = #{createTime},</if>
            <if test="updateBy != null">update_by = #{updateBy},</if>
            <if test="updateTime != null">update_time = #{updateTime},</if>
            <if test="remark != null">remark = #{remark},</if>
            <if test="tenantId != null">tenant_id = #{tenantId},</if>
        </trim>
        where id = #{id}
        <if test="createBy != null  and createBy != ''"> and create_by = #{createBy}</if>
    </update>

    <delete id="deleteTbDrawManualById" parameterType="Long">
        delete from tb_draw_manual where id = #{id}
    </delete>

    <delete id="deleteTbDrawManualByIds" parameterType="String">
        delete from tb_draw_manual where
        <if test="createBy != null and createBy != ''">  create_by = #{createBy} and</if>
id in
        <foreach item="id" collection="ids" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>

</mapper>